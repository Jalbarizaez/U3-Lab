<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - De click una vez sobre la pestaña iniciar para poder cargar informacion</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Iniciar aqui", "Bienvenida", "Arbol", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Busqueda", "Busqueda", "Arbol")</li>
                    <li>@Html.ActionLink("Compra", "Compra", "Arbol")</li>
                    <li>@Html.ActionLink("In_Orden", "In_Orden", "Arbol")</li>
                    <li>@Html.ActionLink("Pre_Orden", "Pre_Orden", "Arbol")</li>
                    <li>@Html.ActionLink("Post_Orden", "Post_Orden", "Arbol")</li>
                    <li>@Html.ActionLink("Simulacion", "Simulacion", "Arbol")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Eduardo Albarizaez</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
